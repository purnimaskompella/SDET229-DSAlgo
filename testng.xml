<suite name="DSAlgoSuite" parallel="tests" thread-count="3">
    <!-- Chrome Tests -->
    <test name="ChromeTests">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="Runner.TestRunner"/>
            
        </classes>
    </test>

    <!-- Firefox Tests -->
    <test name="FirefoxTests">
